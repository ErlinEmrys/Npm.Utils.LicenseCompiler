		"commander": "14.0.0"
	},
	"devDependencies":
	{
		"@erlinemrys/eslint-config": "0.0.4",
		"@types/node": "22.15.29",
		"eslint": "9.29.0",
		"tsup": "8.5.0",
		"tsx": "4.20.3",
		"typescript": "5.8.3"
